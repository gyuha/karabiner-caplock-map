      "description": "letf_command to left_control",
      "manipulators": [
        {
          "description": "letf_control -> left_command",
          "type": "basic",
          "from": {
            "key_code": "left_control"
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ]
        },
        {
          "description": "letf_command -> left_control",
          "type": "basic",
          "from": {
            "key_code": "left_command"
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ]
        }
      ]
    },
    {
      "description": "CapsLock to Hyper",
      "manipulators": [
        {
          "description": "capslock = esc(click) | hyper(hold)",
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_shift",
              "modifiers": ["right_command", "right_control", "right_option"]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ]
        },
        {
          "description": "language switch - right command",
          "type": "basic",
          "from": {
            "key_code": "right_command"
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ]
        },
        {
          "description": "language switch = right option",
          "type": "basic",
          "from": {
            "key_code": "right_option"
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ]
        }
      ]
    },
